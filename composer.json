{
    "name": "msouto/wiki",
    "description": "A Laravel Nova tool.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "illuminate/support": "^5.4|^6.0|^7.0|^8.0|^9.0",
        "illuminate/view": "^5.4|^6.0|^7.0|^8.0|^9.0",
        "erusev/parsedown-extra": "^0.8.1",
        "symfony/dom-crawler": "^4.1|^5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0|^8.0|^9.0",
        "laravel/framework": "^5.4|^6.0|^7.0|^8.0|^9.0",
        "orchestra/testbench": "^3.4|^4.0|^5.0"
    },
    "autoload": {
        "psr-4": {
            "Pdmfc\\Wiki\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Pdmfc\\Wiki\\ToolServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
